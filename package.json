    "koa": "2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-cash": "^2.1.1",
    "koa-compress": "^2.0.0",
    "koa-conditional-get": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-csrf": "^3.0.6",
    "koa-error": "^3.1.0",
    "koa-etag": "^3.0.0",
    "koa-file-server": "2",
    "koa-json": "^2.0.2",
    "koa-logger": "^3.0.1",
    "koa-pug": "^3.0.0-2",
    "koa-qs": "^2.0.0",
    "koa-response-time": "^2.0.0",
    "koa-safe-jsonp": "0",
    "koa-session": "^5.5.0",
    "koa-trace": "1",
    "merge-descriptors": "^1.0.0",
    "ms": "2.0.0",
    "mz": "^2.0.0",
    "qs": ">= 2",
    "snyk": "^1.40.2"
  },
  "devDependencies": {
    "istanbul-harmony": "0",
    "mocha": "^3.5.0",
    "supertest": "^3.0.0"
  },
  "engines": {
    "node": ">= 8.0.0"
  },
  "main": "lib",
  "files": [
    "lib",
    "bin",
    "docs",
    "template"
  ],
  "scripts": {
    "test": "NODE_ENV=test mocha --reporter spec --timeout 30s --bail test/app/index.js",
    "test-cov": "NODE_ENV=test node node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --reporter dot --timeout 2m --bail test/app/index.js",
    "test-travis": "NODE_ENV=test node node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- --reporter dot --timeout 2m --bail test/app/index.js",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "keywords": [
    "koa",
    "http",
    "https",
    "http2",
    "framework",
    "generators",
    "es6",
    "modules",
    "promises",
    "push"
  ],
  "snyk": true
